{
  "$schema": "http://json-schema.org/schema#",
  "title": "StatisticsDashboard",
  "type": "object",
  "properties": {
    "dateCut": {
      "type": "string"
    },
    "statistics": {
      "type": "object",
      "properties": {
        "ordersCount": {
          "type": "number"
        },
        "totalComission": {
          "type": "number"
        },
        "totalOrderValue": {
          "type": "number"
        }
      },
      "required": ["ordersCount", "totalComission", "totalOrderValue"]
    }
  },
  "required": ["dateCut", "statistics"],
  "v-indexed": ["dateCut"]
}
