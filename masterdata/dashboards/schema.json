{
  "$schema": "http://json-schema.org/schema#",
  "title": "Dashboard",
  "type": "object",
  "properties": {
    "dateCut": {
      "type": "string"
    },
    "sellers": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "account": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "stats": {
            "type": "object",
            "properties": {
              "ordersCount": {
                "type": "number"
              },
              "totalComission": {
                "type": "number"
              },
              "totalOrderValue": {
                "type": "number"
              }
            },
            "required": ["ordersCount", "totalComission", "totalOrderValue"]
          }
        },
        "required": ["id", "account", "name", "stats"]
      }
    },
    "stats": {
      "type": "object",
      "properties": {
        "ordersCount": {
          "type": "number"
        },
        "dateInvoiced": {
          "type": "string"
        },
        "totalComission": {
          "type": "number"
        },
        "totalOrderValue": {
          "type": "number"
        }
      },
      "required": [
        "ordersCount",
        "dateInvoiced",
        "totalComission",
        "totalOrderValue"
      ]
    }
  },
  "required": ["dateCut", "sellers", "stats"],
  "v-indexed": ["dateCut"]
}
